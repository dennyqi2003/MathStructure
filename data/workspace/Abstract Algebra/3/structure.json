{
    "structure": [
        {
            "type": "Hint",
            "text": "Theorem"
        },
        {
            "type": "Fix",
            "variable": [
                "$G_1$",
                "$G_2$",
                "$\\phi$"
            ],
            "condition": [
                "$G_1, G_2$ are groups",
                "$\\phi : G_1 \\to G_2$ is a group homomorphism"
            ],
            "scope": [
                {
                    "type": "Define",
                    "term": "$\\ker(\\phi)$",
                    "definition": "$\\ker(\\phi)$ is the kernel of $\\phi$"
                },
                {
                    "type": "Show",
                    "proposition": [
                        "$\\mathrm{Img}(\\phi) \\cong G_1 / \\ker(\\phi)$"
                    ],
                    "method": null,
                    "scope": [
                        {
                            "type": "Define",
                            "term": "$K$",
                            "definition": "$K = \\ker(\\phi)$"
                        },
                        {
                            "type": "Have",
                            "claim": [
                                "$G_1 / K$ exists"
                            ],
                            "reason": [
                                "Kernel is Normal Subgroup of Domain"
                            ]
                        },
                        {
                            "type": "Define",
                            "term": "$\\theta$",
                            "definition": "the mapping $\\theta : G_1 / K \\to G_2$ such that $\\forall x \\in G_1, \\theta(xK) = \\phi(x)$"
                        },
                        {
                            "type": "Show",
                            "proposition": [
                                "$\\theta$ is well-defined, i.e., $\\forall x,y \\in G_1 : xK = yK \\implies \\theta(xK) = \\theta(yK)$"
                            ],
                            "method": null,
                            "scope": [
                                {
                                    "type": "Fix",
                                    "variable": [
                                        "$x$",
                                        "$y$"
                                    ],
                                    "condition": [
                                        "$x, y \\in G_1$",
                                        "$xK = yK$"
                                    ],
                                    "scope": [
                                        {
                                            "type": "LogicChain",
                                            "initial_proposition": [
                                                "$xK = yK$"
                                            ],
                                            "step": [
                                                {
                                                    "operator": "⇔",
                                                    "proposition": [
                                                        "$x^{-1} y \\in K = \\ker(\\phi)$"
                                                    ],
                                                    "reason": [
                                                        "Left Cosets are Equal iff Product with Inverse in Subgroup"
                                                    ]
                                                },
                                                {
                                                    "operator": "⇔",
                                                    "proposition": [
                                                        "$\\phi(x^{-1} y) = e_{G_2}$"
                                                    ],
                                                    "reason": null
                                                },
                                                {
                                                    "operator": "⇔",
                                                    "proposition": [
                                                        "$(\\phi(x))^{-1} \\phi(y) = e_{G_2}$"
                                                    ],
                                                    "reason": null
                                                },
                                                {
                                                    "operator": "⇔",
                                                    "proposition": [
                                                        "$\\phi(x) = \\phi(y)$"
                                                    ],
                                                    "reason": null
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "Have",
                            "claim": [
                                "$\\theta$ is well-defined"
                            ],
                            "reason": null
                        },
                        {
                            "type": "CalculationChain",
                            "initial_expression": [
                                "$\\mathrm{Img}(\\theta)$"
                            ],
                            "step": [
                                {
                                    "operator": "=",
                                    "expression": [
                                        "\\{\\theta(xK) : x \\in G_1\\}"
                                    ],
                                    "reason": null
                                },
                                {
                                    "operator": "=",
                                    "expression": [
                                        "\\{\\phi(x) : x \\in G_1\\}"
                                    ],
                                    "reason": null
                                },
                                {
                                    "operator": "=",
                                    "expression": [
                                        "\\mathrm{Img}(\\phi)"
                                    ],
                                    "reason": null
                                }
                            ]
                        },
                        {
                            "type": "Show",
                            "proposition": [
                                "$\\theta$ is a homomorphism"
                            ],
                            "method": null,
                            "scope": [
                                {
                                    "type": "Fix",
                                    "variable": [
                                        "x",
                                        "y"
                                    ],
                                    "condition": [
                                        "x, y \\in G_1"
                                    ],
                                    "scope": [
                                        {
                                            "type": "CalculationChain",
                                            "initial_expression": [
                                                "\\theta(xK yK)"
                                            ],
                                            "step": [
                                                {
                                                    "operator": "=",
                                                    "expression": [
                                                        "\\theta(xyK)"
                                                    ],
                                                    "reason": null
                                                },
                                                {
                                                    "operator": "=",
                                                    "expression": [
                                                        "\\phi(xy)"
                                                    ],
                                                    "reason": null
                                                },
                                                {
                                                    "operator": "=",
                                                    "expression": [
                                                        "\\phi(x) \\phi(y)"
                                                    ],
                                                    "reason": null
                                                },
                                                {
                                                    "operator": "=",
                                                    "expression": [
                                                        "\\theta(xK) \\theta(yK)"
                                                    ],
                                                    "reason": null
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "type": "Have",
                            "claim": [
                                "$\\theta$ is a monomorphism whose image equals $\\mathrm{Img}(\\phi)$"
                            ],
                            "reason": null
                        }
                    ]
                }
            ]
        }
    ]
}
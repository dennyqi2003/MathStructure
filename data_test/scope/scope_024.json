                      ],
                      "reasons": [
                        "前一步不等式",
                        "$|x-x_0|<\\delta\\le\\sqrt{x_0(1-x_0)}\\,\\epsilon$"
                      ],
                      "type": "Have"
                    }
                  ],
                  "type": "Fix",
                  "var_list": [
                    "x"
                  ]
                }

    {
      "proposition": [
        "$f(x)=\\sqrt{x(1-x)}$ 在闭区间$[0,1]$上连续"
      ],
      "scope": [
        {
          "comment": "Proof",
          "type": "Hint"
        },
        {
          "condition": [
            "$x_0\\in(0,1)$"
          ],
          "scope": [
            {
              "meaning": [
                "\"$\\eta=\\min\\{x_0,1-x_0\\}>0$\""
              ],
              "symbol": "\\eta",
              "type": "Define"
            },
            {
              "condition": [
                "$x\\in(0,1)$",
                "$|x-x_0|<\\eta$"
              ],
              "scope": [
                {
                  "proposition": [
                    "$|\\sqrt{x(1-x)}-\\sqrt{x_0(1-x_0)}|=\\dfrac{|(1-x)-(1-x_0)|\\,|x-x_0|}{\\sqrt{x(1-x)}+\\sqrt{x_0(1-x_0)}}<\\dfrac{1}{\\sqrt{x_0(1-x_0)}}|x-x_0|"
                  ],
                  "reasons": [],
                  "type": "Have"
                }
              ],
              "type": "Fix",
              "var_list": [
                "x"
              ]
            },
            {

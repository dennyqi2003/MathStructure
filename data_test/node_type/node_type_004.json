{
  "ProofStructure": [
    {
      "proposition": [
        "there exists N in ℕ such that for all n > N, x_n < y_n"
      ],
      "type": "Show"
    },
    {
      "condition": [
        "{x_n} convergent and \\lim_{n\\to\\infty} x_n = A",
        "{y_n} convergent and \\lim_{n\\to\\infty} y_n = B"
      ],
      "type": "Fix",
      "var_list": [
        "x",
        "y",
        "A",
        "B"
      ]
    },
    {
      "assumption": [
        "A < B"
      ],
      "type": "Assume"
    },
    {
      "condition": [
        "A < C < B"
      ],
      "type": "Fix",
      "var_list": [
        "C"
      ]
    },
    {
      "condition": [
        "∀ n > N', |x_n - A| < C - A"
      ],
      "type": "Fix",
      "var_list": [
        "N'"
      ]
    },
    {
      "proposition": [
        "for all n > N', x_n < C"
      ],
      "reasons": [
        "|x_n - A| < C - A"
      ],
      "type": "Have"
    },
    {
      "condition": [
        "∀ n > N'', |y_n - B| < B - C"
      ],
      "type": "Fix",
      "var_list": [
        "N''"
      ]
    },
    {
      "proposition": [
        "for all n > N'', y_n > C"
      ],
      "reasons": [
        "|y_n - B| < B - C"
      ],
      "type": "Have"
    },
    {
      "meaning": [
        "N = \\max\\{N', N''\\}"
      ],
      "symbol": "N",
      "type": "Define"
    },
    {
      "proposition": [
        "for all n > N, x_n < y_n"
      ],
      "reasons": [
        "n > N implies n > N' and n > N''",
        "x_n < C",
        "y_n > C"
      ],
      "type": "Have"
    }
  ]
}
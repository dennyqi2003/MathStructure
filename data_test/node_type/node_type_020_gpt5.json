{
  "ProofStructure": [
    {
      "type": "Define",
      "symbol": "T",
      "meaning": [
        "$T = \\{ x \\mid x \\in \\mathbb{Q} \\text{ and } x > 0, x^2 < 2 \\}$"
      ]
    },
    {
      "type": "Show",
      "proposition": [
        "$T$ has no least upper bound in $\\mathbb{Q}$"
      ]
    },
    {
      "type": "Hint",
      "comment": "By contradiction"
    },
    {
      "type": "Assume",
      "assumption": [
        "$T$ has a least upper bound in $\\mathbb{Q}$"
      ]
    },
    {
      "type": "Fix",
      "var_list": [
        "m",
        "n"
      ],
      "condition": [
        "$m,n\\in \\mathbb{N}^*$",
        "$m,n$ are coprime",
        "$\\sup T=\\frac{n}{m}$"
      ]
    },
    {
      "type": "Have",
      "proposition": [
        "$1<\\left(\\frac{n}{m}\\right)^2<3$"
      ],
      "reasons": [
        "obvious"
      ]
    },
    {
      "type": "Have",
      "proposition": [
        "there are only two possible cases"
      ],
      "reasons": [
        "the square of a rational number cannot be 2"
      ]
    },
    {
      "type": "Assume",
      "assumption": [
        "$1<\\left(\\frac{n}{m}\\right)^2<2$"
      ]
    },
    {
      "type": "Fix",
      "var_list": [
        "t"
      ],
      "condition": [
        "$\\frac{n^2}{m^2}=2-t$",
        "$0<t<1$"
      ]
    },
    {
      "type": "Define",
      "symbol": "r",
      "meaning": [
        "$r=\\frac{n}{6m}$"
      ]
    },
    {
      "type": "Have",
      "proposition": [
        "$n+r>0$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "$\\frac{n+r}{m}\\in \\mathbb{Q}$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "\\frac{n^2}{3m^2}t<\\frac{2}{3},\\quad \\frac{2n}{m}-r<\\frac{n}{18}t"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "\\left(\\frac{n+r}{m}\\right)^2=\\frac{n^2}{m^2}+\\frac{2n}{m}r+r^2=2-t+\\frac{2n}{m}r+r^2<2"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "$\\frac{n+r}{m}\\in T$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "contradicting the assumption that $\\frac{n}{m}$ is the least upper bound of $T$"
      ],
      "reasons": []
    },
    {
      "type": "Assume",
      "assumption": [
        "$2<\\left(\\frac{n}{m}\\right)^2<3$"
      ]
    },
    {
      "type": "Fix",
      "var_list": [
        "t"
      ],
      "condition": [
        "$\\frac{n^2}{m^2}=2+t$",
        "$0<t<1$"
      ]
    },
    {
      "type": "Define",
      "symbol": "r",
      "meaning": [
        "$r=\\frac{n}{6m}$"
      ]
    },
    {
      "type": "Have",
      "proposition": [
        "$n-r>0$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "$\\frac{n-r}{m}\\in \\mathbb{Q}$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "\\frac{2n}{m}-r<\\frac{n^2}{3m^2}t<1"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "\\left(\\frac{n-r}{m}\\right)^2=\\frac{n^2}{m^2}-\\frac{2n}{m}r+r^2=2+t-\\frac{2n}{m}r+r^2>2"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "$\\frac{n-r}{m}\\in T$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "contradicting the assumption that $\\frac{n}{m}$ is the least upper bound of $T$"
      ],
      "reasons": []
    },
    {
      "type": "Have",
      "proposition": [
        "$T$ has no least upper bound in $\\mathbb{Q}$"
      ],
      "reasons": []
    }
  ]
}
{
  "ProofStructure": [
    {
      "proposition": [
        "$\\,$If there exists $r>0$ such that for $0<|x-x_0|<r$, $g(x)\\leq f(x)\\leq h(x)$, and $\\lim_{x\\to x_0} g(x)=\\lim_{x\\to x_0} h(x)=A$, then $\\lim_{x\\to x_0} f(x)=A$"
      ],
      "type": "Show"
    },
    {
      "condition": [
        "$r>0$",
        "for $0<|x-x_0|<r,\\ g(x)\\leq f(x)\\leq h(x)$",
        "$\\lim_{x\\to x_0} g(x)=\\lim_{x\\to x_0} h(x)=A$"
      ],
      "type": "Fix",
      "var_list": [
        "f",
        "g",
        "h",
        "x_0",
        "A",
        "r"
      ]
    },
    {
      "proposition": [
        "$\\lim_{x\\to x_0} f(x)=A$"
      ],
      "type": "Show"
    },
    {
      "condition": [
        "$\\epsilon>0$"
      ],
      "type": "Fix",
      "var_list": [
        "\\epsilon"
      ]
    },
    {
      "proposition": [
        "there exists $\\delta_1>0$ such that for all $x$ with $0<|x-x_0|<\\delta_1$, $|h(x)-A|<\\epsilon$"
      ],
      "reasons": [
        "$\\lim_{x\\to x_0} h(x)=A$"
      ],
      "type": "Have"
    },
    {
      "proposition": [
        "$|h(x)-A|<\\epsilon \\implies h(x)<A+\\epsilon$"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "proposition": [
        "there exists $\\delta_2>0$ such that for all $x$ with $0<|x-x_0|<\\delta_2$, $|g(x)-A|<\\epsilon$"
      ],
      "reasons": [
        "$\\lim_{x\\to x_0} g(x)=A$"
      ],
      "type": "Have"
    },
    {
      "proposition": [
        "$|g(x)-A|<\\epsilon \\implies A-\\epsilon<g(x)$"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "meaning": [
        "$\\delta=\\min\\{\\delta_1,\\delta_2,r\\}$"
      ],
      "symbol": "$\\delta$",
      "type": "Define"
    },
    {
      "proposition": [

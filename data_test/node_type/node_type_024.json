{
  "ProofStructure": [
    {
      "proposition": [
        "$f(x)=\\sqrt{x(1-x)}$ 在闭区间$[0,1]$上连续"
      ],
      "type": "Show"
    },
    {
      "comment": "Proof",
      "type": "Hint"
    },
    {
      "condition": [
        "$x_0\\in(0,1)$"
      ],
      "type": "Fix",
      "var_list": [
        "x_0"
      ]
    },
    {
      "meaning": [
        "$\\eta=\\min\\{x_0,1-x_0\\}>0$"
      ],
      "symbol": "\\eta",
      "type": "Define"
    },
    {
      "condition": [
        "$x\\in(0,1)$",
        "$|x-x_0|<\\eta$"
      ],
      "type": "Fix",
      "var_list": [
        "x"
      ]
    },
    {
      "proposition": [
        "$|\\sqrt{x(1-x)}-\\sqrt{x_0(1-x_0)}|=\\dfrac{|(1-x)-(1-x_0)|\\,|x-x_0|}{\\sqrt{x(1-x)}+\\sqrt{x_0(1-x_0)}}<\\dfrac{1}{\\sqrt{x_0(1-x_0)}}|x-x_0|"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "condition": [
        "$\\epsilon>0$"
      ],
      "type": "Fix",
      "var_list": [
        "\\epsilon"
      ]
    },
    {
      "meaning": [
        "$\\delta=\\min\\{\\eta,\\sqrt{x_0(1-x_0)}\\,\\epsilon\\}$"
      ],
      "symbol": "\\delta",
      "type": "Define"
    },
    {
      "condition": [
        "$|x-x_0|<\\delta$"
      ],
      "type": "Fix",
      "var_list": [
        "x"
      ]
    },
    {
      "proposition": [
        "$|\\sqrt{x(1-x)}-\\sqrt{x_0(1-x_0)}|<\\dfrac{1}{\\sqrt{x_0(1-x_0)}}|x-x_0|<\\epsilon$"
      ],
      "reasons": [
        "前一步不等式",
        "$|x-x_0|<\\delta\\le\\sqrt{x_0(1-x_0)}\\,\\epsilon$"
      ],
      "type": "Have"
    },
    {
      "proposition": [
        "$f(x)$ 在$(0,1)$上连续"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "comment": "Now consider the endpoints.",
      "type": "Hint"
    },
    {
      "condition": [
        "$\\epsilon>0$"
      ],
      "type": "Fix",
      "var_list": [
        "\\epsilon"
      ]
    },
    {
      "meaning": [
        "$\\delta=\\epsilon^2$"
      ],
      "symbol": "\\delta",
      "type": "Define"
    },
    {
      "condition": [
        "$0\\le x<\\delta$"
      ],
      "type": "Fix",
      "var_list": [
        "x"
      ]
    },
    {
      "proposition": [
        "$|f(x)-f(0)|\\le\\sqrt{x}<\\epsilon$"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "condition": [
        "$1-\\delta<x\\le1$"
      ],
      "type": "Fix",
      "var_list": [
        "x"
      ]
    },
    {
      "proposition": [
        "$|f(x)-f(1)|\\le\\sqrt{1-x}<\\epsilon$"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "proposition": [
        "$f(x)$ 在$x=0$右连续且在$x=1$左连续"
      ],
      "reasons": [],
      "type": "Have"
    },
    {
      "proposition": [
        "$f(x)=\\sqrt{x(1-x)}$ 在闭区间$[0,1]$上连续"
      ],
      "reasons": [],
      "type": "Have"
    }
  ]
